<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.quickeats">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/Theme.Quickeats">

        <activity
            android:name=".splashactivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".activity_splash2" android:exported="false" />
        <activity android:name=".activity_start" android:exported="false" />
        <activity android:name=".activity_login" android:exported="false" />
        <activity android:name=".activity_signup" android:exported="false" />
        <activity android:name=".activity_home" android:exported="false" />

    </application>
</manifest>